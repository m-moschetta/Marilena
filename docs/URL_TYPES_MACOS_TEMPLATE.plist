<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleURLTypes</key>
    <array>
        <dict>
            <key>CFBundleURLName</key>
            <string>com.yourcompany.marilena-mac.oauth</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <!-- Sostituisci con lo schema del tuo client ID macOS (Google OAuth).
                     Esempio comune per Google: reversed client ID (p.es. com.googleusercontent.apps.1234567890-abc123) -->
                <string>com.googleusercontent.apps.YOUR_MAC_CLIENT_ID_REVERSED</string>
            </array>
            <key>CFBundleTypeRole</key>
            <string>Editor</string>
        </dict>
    </array>
</dict>
</plist>

